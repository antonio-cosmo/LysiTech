<section>
  <p style="color: green"><%= notice %></p>

  <h1>Profiles</h1>

  <div id="profiles">
    <% @profiles.each do |profile| %>
      <%= render profile %>
      <p>
        <%= link_to "Mostrar usuario", profile %>
      </p>
    <% end %>
  </div>

  <%= link_to "Criar usuario", new_profile_path %>
</section>
